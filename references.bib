  author = {Cole, James R. and Wang, Qiong and Fish, Jordan A. and Chai, Benli and McGarrell, Donna M. and Sun, Yanni and Brown, C. Titus and {Porras-Alfaro}, Andrea and Kuske, Cheryl R. and Tiedje, James M.},
