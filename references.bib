@article{cuddington_challenges_2023,
  title = {Challenges and Opportunities to Build Quantitative Self-Confidence in Biologists},
  author = {Cuddington, Kim and Abbott, Karen C and Adler, Frederick R and Aydeniz, Mehmet and Dale, Rene and Gross, Louis J and Hastings, Alan and Hobson, Elizabeth A and Karatayev, Vadim A and Killion, Alexander and Madamanchi, Aasakiran and Marraffini, Michelle L and McCombs, Audrey L and Samyono, Widodo and Shiu, Shin-Han and Watanabe, Karen H and White, Easton R},
